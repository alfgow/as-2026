# # Backend/admin/.htaccess

 RewriteEngine On
# # # Redirige todo (menos archivos/directorios reales) a router.php
 RewriteCond %{REQUEST_FILENAME} !-f
 RewriteCond %{REQUEST_FILENAME} !-d
 RewriteRule ^(.*)$ router.php [QSA,L]

